{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Dynamic Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}"
        },
        {
            "name": "Python Debugger: Current File with Fixed Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "--tokenizer_file ~/private/experiments/name_generator/human_names_tokenizer_n=3_20251109.0/token_dict.n=3.json --names_file ~/private/data/finnlp_humannames/human_names_list.txt --epochs=1 --batch_size=32"
        }
    ]
}